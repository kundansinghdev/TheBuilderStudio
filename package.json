{
  "name": "the-builder-studio",
  "version": "1.0.0",
  "private": true,
  "description": "Enterprise-level SaaS platform for rapid MVP development and startup building",
  "keywords": [
    "saas",
    "mvp",
    "startup",
    "development",
    "enterprise",
    "scalable"
  ],
  "author": "The Builder Studio Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "analyze": "ANALYZE=true npm run build",
    "clean": "rm -rf .next out dist",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "docker:build": "docker build -t the-builder-studio .",
    "docker:run": "docker run -p 3000:3000 the-builder-studio",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts"
  },
